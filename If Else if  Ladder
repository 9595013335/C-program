Q. wap to calculate Student Result.


#include<stdio.h>

int main()
{
    int marks;
    printf("Enter mark of student  ");
    scanf("%d",&marks);
    
    if(marks==100)
    {
        printf("\nStudent is Geneuis");
        
    }
    else if(marks>=80 &&  marks<100)
    {
        printf(" \nStudent is Topper");
    }
    else if(marks>=65 && marks<=79)
    {
        printf("\n Student is Average");
        
    }
    else
    {
        printf("\n Better Luck Next Time");
    }
    
}
